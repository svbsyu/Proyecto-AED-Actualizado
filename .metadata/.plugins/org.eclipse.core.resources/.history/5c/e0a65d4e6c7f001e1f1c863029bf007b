package Clases;
public class Alumno extends Docente{
	public int codigoAlumno;
	public Alumno(int codigoAlumno, String nombre, String apellido, String telefono, String dni) {
		super(codigoAlumno, nombre, apellido, telefono, dni, codigoAlumno);
		this.codigoAlumno=codigoAlumno;
	}
	public int getCodigoAlumno() {
		return codigoAlumno;
	}
	public void setCodigoAlumno(int codigoAlumno) {
		this.codigoAlumno = codigoAlumno;
	}
	
}
